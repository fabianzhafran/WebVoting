
<div id="header">
    <img src="/images/logo.png" alt="logo" />
    <img src="/images/logo2.png" alt="logo" />
    <h3>e-Vote Psikologi Universitas Kristen Maranatha</h3>
</div>

<!-- <section id='identitas'> -->
<div class="container-fluid" id="login-page">
    <div class="row justify-content-center" id="login-row">
        <div class="col-11 col-sm-9 col-md-7 col-lg-5 col-xl-4 my-auto1">
        <% include ./partials/messages %>
        <form class="card form-container" action="/users/register" method="POST">
            <div class="text-center">
                <h3>Registrasi Voting</h3>
            </div>
            
            <div class="form-group">
            <label for="name">Name</label>
            <input
                type="name"
                id="name"
                name="name"
                class="form-control"
                placeholder="Enter Name"
                value="<%= typeof name != 'undefined' ? name : '' %>"
            />
            </div>
            <div class="form-group">
            <label for="email">Email</label>
            <input
                type="email"
                id="email"
                name="email"
                class="form-control"
                placeholder="Enter Email"
                value="<%= typeof email != 'undefined' ? email : '' %>"
            />
            </div>
            <div class="form-group">
            <label for="nrp">NRP</label>
            <input
                type="nrp"
                id="nrp"
                name="nrp"
                class="form-control"
                placeholder="Enter NRP"
                value="<%= typeof nrp != 'undefined' ? nrp : '' %>"
            />
            </div>
            <button type="submit" class="btn btn-primary btn-block">
            Register
            </button>        
            <p class="lead mt-4">
                Jika sudah punya akun, <a href="/users/login">login</a>
            </p>
        </form>


        </div>
    </div>
</div>
<!-- </section> -->